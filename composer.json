{
    "name": "plusforta/soapbundle",
    "description": "SOAP for R+V Api",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Jan Hentschel",
            "email": "jan.hentschel@plusforta.de"
        }
    ],
    "autoload": {
        "psr-4": {
            "PlusForta\\RuVSoapBundle\\": "src/"
        }
    },
    "repositories": [
        {
