{
    "name": "plusforta/soapbundle",
    "description": "SOAP for R+V Api",
    "type": "symfony-bundle",
    "version": "1.01",
    "authors": [
        {
            "name": "Jan Hentschel",
            "email": "jan.hentschel@plusforta.de"
        },
        {
            "name": "Marc Runkel",
            "email": "marc.runkel@plusforta.de"
        }
    ],
    "autoload": {
