{
    "name": "plusforta/soapbundle",
    "description": "SOAP for R+V Api",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Anar Haqverdiyev@plusforta.de",
            "email": "anar.haqverdiyev@plusforta.de"
        },
        {
            "name": "Jan Hentschel",
            "email": "jan.hentschel@plusforta.de"
        },
        {
            "name": "Marc Runkel",
            "email": "marc.runkel@plusforta.de"
        }
    ],
    "autoload": {
        "psr-4": {
            "PlusForta\\RuVSoapBundle\\": "src/"
        }
    },
    "require": {
        "phpro/soap-client": "^2.1.0",
        "webmozart/assert": "^1.5",
        "symfony/http-client": "^6.0",
        "nyholm/psr7": "^1.4",
        "ext-mbstring": "*",
        "php": ">=8.2"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "symfony/config": "^6.2",
        "symfony/dependency-injection": "^6.2",
        "vimeo/psalm": "^5.9.0",
        "symfony/http-kernel": "^6.2",
        "php-parallel-lint/php-parallel-lint": "^v1.3.2",
        "symplify/easy-coding-standard": "^12.3",
        "phpro/grumphp": "^1.16"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "php-http/discovery": true,
            "phpro/grumphp": true
        }
    }
}
