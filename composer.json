    "require": {
        "phpro/soap-client": "^1.1.2",
        "webmozart/assert": "^1.5"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "symfony/config": "^4.3",
        "symfony/dependency-injection": "^4.3",
        "vimeo/psalm": "^3.5",
        "symfony/http-kernel": "^4.3",
        "zendframework/zend-code": "3.1.0"
    }
}
