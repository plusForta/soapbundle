        "symfony/dependency-injection": "^5.3",
        "vimeo/psalm": "^3.5",
        "symfony/http-kernel": "^5.3",
        "php-parallel-lint/php-parallel-lint": "^1.2"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    }
